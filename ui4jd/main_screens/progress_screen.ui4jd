          "controls": [
            {
              "pointer_text_tip_panel@common_4jd.global_label": {
                "shadow": true,
                "anchor_from": "top_middle",
                "anchor_to": "top_middle",
                "text_alignment": "center",
                "size": [ "100%", "default" ],
                "text": "#progress_text",
                "bindings": [
                  { "binding_name": "#progress_text" }
                ]
              }
            }
          ]
        }
      },
      {
        "text_hold_2": {
          "type": "panel",
          "size": [ "100%-24px", 10 ],
          "anchor_from": "top_middle",
          "anchor_to": "top_middle",
          "offset": [ 0, 24 ],
          "clips_children": true,
          "bindings": [
            {
              "binding_name": "(not #loading_bar_visible)",
